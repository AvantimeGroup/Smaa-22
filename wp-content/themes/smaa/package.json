  },
  "devDependencies": {
    "node-sass": "^4.13.1",
    "@wordpress/scripts": "^7.2.0"
  },
  "scripts": {
    "compile:css": "node-sass --include-path scss sass/style.sass style.css && node-sass --include-path scss sass/style.sass editor-style.css",
    "lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "scss": "node-sass --include-path scss/** sass/style.sass style.css --output-style=expanded --indent-type=tab --indent-width=1 -rw && node-sass --include-path scss/** sass/style.sass editor-style.css --output-style=expanded --indent-type=tab --indent-width=1 -rw",
    "build": "wp-scripts build",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "lint:css": "wp-scripts lint-style",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "start": "wp-scripts start",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "format:js": "wp-scripts format-js",
    "format:js:src": "wp-scripts format-js ./src"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.4.1",
    "install": "^0.13.0",
    "npm": "^6.14.15",
    "popper.js": "^1.16.1"
  }
}
